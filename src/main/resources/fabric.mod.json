{
  "schemaVersion": 1,
  "id": "vanilla-overhaul",
  "version": "${version}",

  "name": "Vanilla Overhaul",
  "description": "A few changes which I prefer to have in my game, that I'm sharing with you! There are further optional changes which can be enabled inside of the config file. To view the changelog, view the README.md from the project's source!",
  "authors": [
    "Lucas Warwick (HermitTheFrog)"
  ],
  "contact": {
    "homepage": "https://lucaswarwick02.github.io/online-resume/",
    "sources": "https://github.com/lucaswarwick02/vanilla-overhaul"
  },

  "license": "CC0-1.0",
  "icon": "assets/vanilla-overhaul/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "lucaswarwick02.VanillaOverhaulMod"
    ]
  },
  "mixins": [
    "vanilla-overhaul.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.20.1",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
